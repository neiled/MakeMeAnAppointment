#holiday_calendar
  %table
    %th
    - while @total > 0
      - HolidayCalendarCell::DAYS_IN_A_WEEK.each do |d|
        = raw("<th>" + d + "</th>") unless @total < 0
        - @total -= 1
    - HolidayCalendarCell::MONTHS_IN_A_YEAR.each do |m|
      %tr
        = raw(calendar_row(m, @current_year, @business_calendar))
